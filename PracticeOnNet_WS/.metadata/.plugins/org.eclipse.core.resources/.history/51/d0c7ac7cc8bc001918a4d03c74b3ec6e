<script type="text/javascript">
	$(function() {
		$('#themes a')
				.click(
						function() {
							var theme = $(this).attr('href');
							$('head')
									.append(
											'<link href="' + theme + '" rel="Stylesheet" type="text/css" />');
							return false;
						});
	});
</script>


