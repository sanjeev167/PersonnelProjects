#Configure SSL. Either https or http connector can be configured here at a time.
#For configuring https and http both, one has to be configured programmatically. 

#server.port: 8443
#server.ssl.key-store: keystore.p12
#server.ssl.key-store-password: mypassword
#server.ssl.keyStoreType: PKCS12
#server.ssl.keyAlias: tomcat

#Set session time out
server.servlet.session.timeout=1m
#spring.main.banner-mode=off

#This will force to select any free port for running server

#server.port=0


